{
    "name": "andreaskviby/laravel-jetstream-tallstack-ai-powered",
    "description": "Laravel Jetstream TALL Stack starter kit with AI-powered features, OTP authentication, and enhanced team management",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jetstream",
        "tall-stack",
        "livewire",
        "alpine",
        "tailwind",
        "otp",
        "authentication",
        "teams",
        "ai-powered"
    ],
    "authors": [
        {
            "name": "Andreas Kviby",
            "email": "andreas@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php setup/installer.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
